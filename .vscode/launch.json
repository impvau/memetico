    "version": "0.2.0",
    "configurations": [
        {
            "name": "CFR Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/main",
            "args": [
                
                "-s","1433120488"
                ,"-t", "${workspaceFolder}/data/data.csv"
                ,"-T", "${workspaceFolder}/data/data.csv"
                ,"-lt", "${workspaceFolder}/out/"
                ,"-f", "2"
                ,"-g", "200"
                ,"-ld", "0.2"
                ,"-dd", "1"
                ,"-lt", "${workspaceFolder}/out"

                //"-d", "0.1",
                //"-f", "4",
                //"-sd", "0.75",
                //"-mr", "0.1",
                //"-p", "0.35"
                //

            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Main"
        },
        {
            "name": "CFR Tests",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/main",
            "args": [
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Tests"
        },
        {
            "name": "Python Data Plots",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/postproc/regen.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Evolution Plots",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/postproc/evolve.py",
            "console": "integratedTerminal"
        },
    ]
}